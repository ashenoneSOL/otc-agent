{
  "$schema": "../../packages/config/jeju-manifest.schema.json",
  "name": "otc-agent",
  "displayName": "OTC Trading Desk",
  "version": "1.0.0",
  "type": "vendor",
  "description": "AI-powered EVM OTC trading desk with Eliza integration. Supports Base, BSC, Jeju mainnet and testnets with cryptographic quote verification and lockup scheduling.",
  "commands": {
    "dev": "bun run dev",
    "build": "bun run build",
    "start": "bun run start",
    "test": "bun run test"
  },
  "ports": {
    "main": 5005,
    "db": 5439,
    "worker": 5137
  },
  "dependencies": ["contracts", "config"],
  "optional": true,
  "enabled": true,
  "autoStart": false,
  "tags": ["otc", "trading", "ai", "eliza", "evm", "base", "bsc", "jeju", "defi"],
  "healthCheck": {
    "url": "http://localhost:5005",
    "interval": 30000
  },
  "agent": {
    "enabled": true,
    "a2aEndpoint": "/api/a2a",
    "tags": ["jeju-app", "otc", "trading", "ai", "multi-chain"],
    "metadata": {
      "category": "defi",
      "provider": "jeju-network",
      "chains": ["base", "bsc", "jeju"]
    },
    "trustModels": ["open", "staked"],
    "x402Support": true
  },
  "networks": {
    "evm": {
      "mainnet": ["base", "bsc", "jeju"],
      "testnet": ["base-sepolia", "bsc-testnet", "jeju-testnet"]
    }
  }
}


